{

	"manifest_version": 2,
	"name": "NSA Mode",
	"description": "Provides advanced NSA-only features for Google Maps",
	"version": "1.0",
	"applications": {
		"gecko": {
			"strict_min_version": "60.0a1"
		}
	},

	"content_scripts": [
		{
			"matches": ["https://*.google.com/*"],
			"js": ["content-script.js"]
		}
	]

}
